{
  "name": "healthycoaching-id",
  "version": "1.0.0",
  "private": true,
  "description": "Health & wellness coaching app untuk pasar Indonesia dengan database makanan lokal",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "test:android": "npm run test:android:unit && npm run test:android:instrumented",
    "test:android:unit": "jest --testPathPattern=unit/android",
    "test:android:instrumented": "cd android && ./gradlew connectedAndroidTest",
    "test:android:performance": "jest --testPathPattern=performance/android",
    "test:android:compatibility": "./scripts/test-device-compatibility.sh",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "android:setup": "./scripts/setup-android.sh",
    "android:device": "react-native run-android --device",
    "android:emulator": "react-native run-android",
    "android:debug": "cd android && ./gradlew assembleDebug && ./gradlew installDebug",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "android:log": "adb logcat -s ReactNativeJS,ReactNative:V8,ReactNative:JSC",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "build:android:optimize": "./scripts/optimize-android-bundle.sh",
    "deploy:android:internal": "./scripts/deploy-internal.sh",
    "deploy:android:production": "./scripts/deploy-production.sh",
    "clean": "react-native clean",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "rm -rf ios/build"
  },
  "keywords": [
    "health",
    "fitness",
    "indonesia",
    "nutrition",
    "workout",
    "react-native",
    "typescript",
    "makanan-indonesia",
    "coaching"
  ],
  "author": "Neima Silk <neimasilk@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neimasilk/healthycoaching-id.git"
  },
  "bugs": {
    "url": "https://github.com/neimasilk/healthycoaching-id/issues"
  },
  "homepage": "https://github.com/neimasilk/healthycoaching-id#readme",
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-safe-area-context": "4.7.4",
    "react-native-screens": "3.27.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "react-hook-form": "^7.48.2",
    "@react-native-async-storage/async-storage": "1.19.5",
    "react-native-sqlite-storage": "^6.0.1",
    "axios": "^1.6.2",
    "react-native-svg": "13.14.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-vector-icons": "^10.0.2",
    "date-fns": "^2.30.0",
    "uuid": "^9.0.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-device-info": "^10.11.0",
    "react-native-permissions": "^3.10.1",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/analytics": "^18.7.3",
    "@react-native-firebase/crashlytics": "^18.7.3",
    "@react-native-firebase/performance": "^18.7.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.42",
    "@types/react-native": "^0.72.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "metro-react-native-babel-preset": "0.77.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "react-native-svg-transformer": "^1.1.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-.*)/"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.{js,jsx,ts,tsx}"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/main.tsx",
      "!src/**/*.stories.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}