# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# API Configuration
REACT_APP_API_URL=https://api.healthycoaching.id
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=10000

# Features
REACT_APP_ENABLE_AI_RECOMMENDATIONS=true
REACT_APP_ENABLE_PAYMENT=true
REACT_APP_ENABLE_SOCIAL=true
REACT_APP_ENABLE_ANALYTICS=true

# Indonesian Specific
REACT_APP_DEFAULT_LOCALE=id-ID
REACT_APP_DEFAULT_CURRENCY=IDR
REACT_APP_ENABLE_HALAL_FILTER=true
REACT_APP_INDONESIAN_FOOD_DB_SIZE=1000

# Development
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_FLIPPER=true

# Performance
REACT_APP_BUNDLE_OPTIMIZATION=true
REACT_APP_IMAGE_OPTIMIZATION=true
REACT_APP_CACHE_EXPIRY=3600

# Payment Methods
REACT_APP_GOPAY_ENABLED=true
REACT_APP_OVO_ENABLED=true
REACT_APP_DANA_ENABLED=true
REACT_APP_BANK_TRANSFER_ENABLED=true

# Build Configuration
REACT_APP_VERSION_NAME=1.0.0
REACT_APP_VERSION_CODE=1
REACT_APP_BUILD_NUMBER=1

# Analytics
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_CRASHLYTICS_ENABLED=true
REACT_APP_PERFORMANCE_MONITORING=true